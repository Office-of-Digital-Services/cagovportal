---
landing: Agency
pagination:
    data: state_entity.agencies
    size: 1
    alias: item
    addAllPagesToCollections : true
permalink: "agency/{{ item.AgencyId }}/"
eleventyComputed: 
  description: Agency details for "{{ item.AgencyFullName }}"
  title: "{{ item.AgencyFullName }}"
  keywords: "{{ item.Keywords}}"
  eleventyNavigation:
    key: "{{ item.AgencyName | slugify }}"
    title:  "{{ item.AgencyFullName }}"
    parent: Agencies
---

<!-- Begin Structured Data -->
<script type="application/ld+json">
  {{
    item.structuredData | dump | safe
  }}
</script>
<!-- End Structured Data -->

<div class="row p-b-lg">
  <div class="col-md-10 mx-auto">
    <h2 class="mt-lg-0">
      Agency - {{ item.FriendlyName }} ({{ item.Acronym }})
    </h2>

    {% if item.LogoUrl %}
    <img src="{{item.structuredData.logo}}" style="width:50%" />

    <br />
    {% endif %}

    <h2 class="mt-lg-0">Services</h2>
    {%- for service in state_entity.services | pluck('AgencyId',item.AgencyId)
    -%}
    <a href="/agency/{{ service.AgencyId }}/service/{{ service.ServiceId }}/"
      >{{service.ServiceName}}</a
    ><br />
    {%- endfor %}
    <hr />
    <pre>
{{ item | dump(2) }}
    </pre>
  </div>
</div>
