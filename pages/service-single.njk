---
landing: Service
pagination:
    data: state_entity.services
    size: 1
    alias: item
    addAllPagesToCollections : true
permalink: "agency/{{ item.AgencyId }}/service/{{ item.ServiceId }}/"
eleventyComputed: 
  description: Service details for "{{ item.ServiceName }}"
  keywords: "{{ item.Keywords }}"
  title: "{{ item.ServiceName }}"
  eleventyNavigation:
    key: "Service{{ item.ServiceId }}"
    title:  "{{ item.ServiceName }}"
    parent: "Agency{{ item.AgencyId }}"
---

<!-- Begin Structured Data -->
<script type="application/ld+json">
  {{ item.structuredData  | dump | safe }}
</script>
<!-- End Structured Data -->

<div class="row p-b-lg">
  <div class="col-md-10 mx-auto">
    <h2 class="mt-lg-0">Service - {{ item.ServiceName }}</h2>
    {% if item.ImageUrl %}
    <img
      src="https://stateentityprofile.ca.gov/Uploads/{{item.ImageUrl}}"
      style="aspect-ratio: 4 / 3; width:50%" />

    <br />
    {% endif %}
    <p>{{item.Description}}</p>
    <h3 class="mt-lg-0">Agency</h3>

    <p>
      <a href="/agency/{{ item.AgencyId }}/"
        >{{ item.FriendlyName }} ({{ item.Acronym }})</a
      >
    </p>

    <h3 class="mt-lg-0">Service Type</h3>
    <ul>
      {%- for tag in item.ServiceType.split("|") -%}
      <li><a href="/service/?q={{tag | trim}}+">{{ tag | trim }}</a>&nbsp;</li>
      {%- endfor %}
    </ul>

    <h3 class="mt-lg-0">Keywords</h3>
    <p>
      {%- for keyword in item.Keywords.split(",") -%}
      <a href="/service/?q={{keyword | trim}}+">{{ keyword | trim }}</a>&nbsp;
      {%- endfor %}
    </p>

    <h3 class="mt-lg-0">Contact Info</h3>
    <p>Phone <a href="tel:{{item.ContactPhone}}">{{item.ContactPhone}}</a></p>
    <p>URL <a href="{{item.ContactUrl}}">{{item.ContactUrl}}</a></p>

    <h3 class="mt-lg-0">Agency Tags</h3>
    <ul>
      {%- for tag in item.AgencyTags.split("|") -%}
      <li><a href="/service/?q={{tag | trim}}+">{{ tag | trim }}</a>&nbsp;</li>
      {%- endfor %}
    </ul>
    <hr />
    <pre>
{{ item | dump(2) }}
    </pre>
  </div>
</div>
